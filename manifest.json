{
  "manifest_version": 2,
  "name": "samp-browser-extension",
  "description": "SAMP Browser Extension",
  "version": "0.0.2",
  "background": {
    "scripts": [
      "background_script.js"
    ]
  },
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": [
      "content_script.js"
    ]
  }],
  "permissions": [
    "*://localhost/*"
  ]
}
